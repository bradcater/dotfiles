# Taken from
#   http://www.howtoforge.com/anonymous-ssh-sessions-with-tor
Host anon_*
  #CheckHostIP no
  Protocol 2
  ProxyCommand connect -4 -S localhost:9050 $(tor-resolve %h localhost:9050) %p
